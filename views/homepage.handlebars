
  {{#each posts as |post|}}
  <div class="card  my-2">
    <div class="card-header ">
      <span class="float-left"> <a class="card-header-link card-link" href="/api/comments/view/{{post.id}}">{{post.title}}</a></span>
        <span class = "float-right ">Posted by {{post.user.user_name}} on {{format_date post.date_created}} </span>
    </div>
    <div class="card-body rounded">
      
      <p class="card-text">{{post.description}}</p>
    
    </div>
    
  </div>
  {{/each}}
